{
  "manifest_version": 3,
  "name": "Minha Extens√£o",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "https://gamersclub.com.br/lobby-beta"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://gamersclub.com.br/*",
        "https://gamersclub.com.br/lobby-beta"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}